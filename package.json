{
  "name": "mr-brewerywatch-web",
  "version": "0.1.1",
  "main": "index.js",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html -d ./build --cert /etc/nginx/ssl/ck-internal.com.pem --key /etc/nginx/ssl/ck-internal.com.key",
    "start-verbose": "parcel src/index.html --log-level 2 -d ./build",
    "watch": "parcel watch src/index.html -d ./build",
    "dist": "parcel build src/index.html -d ./dist",
    "clean-dist": "rm -rf ./dist",
    "image": "docker build --build-arg target=production --rm=true -t us.gcr.io/dumaine-dev/$npm_package_name:$npm_package_version .",
    "rmi": "docker rmi us.gcr.io/dumaine-dev/$npm_package_name:$npm_package_version || true",
    "push": "docker push us.gcr.io/dumaine-dev/$npm_package_name:$npm_package_version",
    "release": "yarn rmi && yarn dist && npm version patch && yarn image && yarn push"
  },
  "dependencies": {
    "dateformat": "^3.0.3",
    "fast-deep-equal": "^1.1.0",
    "firebase": "^4.12.1",
    "history": "^4.7.2",
    "kea": "^0.28.2",
    "kea-saga": "^0.3.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "querystring": "^0.2.0",
    "react": "^16.0.0-0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "4.1.1",
    "react-md": "^1.3.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "shallowequal": "^1.0.2",
    "ulid": "^2.3.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.7.0",
    "postcss-modules": "^1.1.0",
    "scss-lint": "^0.0.0"
  }
}
